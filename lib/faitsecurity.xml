<FAIT>

  <SUBTYPE NAME='CheckSqlTaint' ENABLE='FALSE' >
     <VALUE NAME='MAYBE_SQLTAINTED' ATTRIBUTES='SqlTainted Tainted' DEFAULT='TRUE' />
     <VALUE NAME='SQLTAINTED' />
     <VALUE NAME='FULLY_SQLTAINTED' />
     <VALUE NAME='UNSQLTAINTED' ATTRIBUTES='SqlUntainted Untainted' CONSTANT='TRUE' UNINIT='TRUE' />

     <DEFAULT PRIMITIVE='TRUE' RESULT='UNSQLTAINTED' />

     <BASE PRIMITIVE='TRUE' RESULT='UNSQLTAINTED' />

     <OPERATION OPERATOR='ADD' AND='TRUE'
	   ARGS='UNSQLTAINTED UNSQLTAINTED' VALUE='UNSQLTAINTED' />
     <OPERATION OPERATOR='ADD' AND='TRUE'
	   ARGS='MAYBE_SQLTAINTED MAYBE_SQLTAINTED' VALUE='MAYBE_SQLTAINTED' />
     <OPERATION OPERATOR='ADD' AND='TRUE'
	   ARGS='MAYBE_SQLTAINTED UNSQLTAINTED' VALUE='MAYBE_SQLTAINTED' />
     <OPERATION OPERATOR='ADD' AND='TRUE'
	   ARGS='UNSQLTAINTED MAYBE_SQLTAINTED' VALUE='MAYBE_SQLTAINTED' />
     <OPERATION OPERATOR='ADD' VALUE='FULLY_SQLTAINTED' />
     <OPERATION ARGS='SQLTAINTED SQLTAINTED' RESULT='SQLTAINTED' VALUE='SQLTAINTED' />
     <OPERATION OPERATOR='STARTINIT DONEINIT' VALUE='UNSQLTAINTED' />
     <OPERATION AND='TRUE' ARGS='UNSQLTAINTED UNSQLTAINTED' VALUE='UNSQLTAINTED' />
     <OPERATION METHOD='*' ARGS='FULLY_SQLTAINTED' RESULT='FULLY_SQLTAINTED' VALUE='FULLY_SQLTAINTED'  />
     <OPERATION METHOD='*' ARGS='SQLTAINTED' RESULT='SQLTAINTED' VALUE='SQLTAINTED' />
     <OPERATION METHOD='*' AND='TRUE' ARGS='UNSQLTAINTED' VALUE='UNSQLTAINTED' />
     <OPERATION RETURN='RESULT' />

     <PREDECESSOR CURRENT='FULLY_SQLTAINTED' PREDECESSOR='FULLY_SQLTAINTED' RESULT='TRUE' />
     <PREDECESSOR CURRENT='FULLY_SQLTAINTED' PREDECESSOR='SQLTAINTED' RESULT='TRUE' />

     <RESTRICT VALUE='SQLTAINTED' WITH='UNSQLTAINTED'
	ERROR='Attempt to use tainted SQL data in a non-tainted location' />
     <RESTRICT VALUE='FULLY_SQLTAINTED' WITH='UNSQLTAINTED'
	ERROR='Attempt to use tainted SQL data in a non-tainted location' />
     <RESTRICT VALUE='SQLTAINTED' WITH='MAYBE_SQLTAINTED' YIELDS='SQLTAINTED' />
     <RESTRICT VALUE='FULLY_SQLTAINTED' WITH='MAYBE_SQLTAINTED' YIELDS='FULLY_SQLTAINTED' />
     <RESTRICT VALUE='FULLY_SQLTAINTED' WITH='SQLTAINTED' YIELDS='FULLY_SQLTAINTED' />
     <RESTRICT VALUE='UNSQLTAINTED' WITH='SQLTAINTED' YIELDS='UNSQLTAINTED' />
     <RESTRICT VALUE='UNSQLTAINTED' WITH='MAYBE_SQLTAINTED' YIELDS='UNSQLTAINTED' />
     <RESTRICT VALUE='MAYBE_SQLTAINTED' WITH='UNSQLTAINTED'
	WARNING='Possible attempt to use tainted SQL data in a non-tainted location' />
     <RESTRICT VALUE='MAYBE_SQLTAINTED' WITH='SQLTAINTED' YIELDS='SQLTAINTED' />

     <MERGE VALUE='SQLTAINTED' WITH='UNSQLTAINTED' YIELDS='SQLTAINTED' />
     <MERGE VALUE='MAYBE_SQLTAINTED' WITH='SQLTAINTED' YIELDS='SQLTAINTED' />
     <MERGE VALUE='MAYBE_SQLTAINTED' WITH='UNSQLTAINTED' YIELDS='MAYBE_SQLTAINTED' />
     <MERGE VALUE='FULLY_SQLTAINTED' WITH='SQLTAINTED' YIELDS='FULLY_SQLTAINTED' />
     <MERGE VALUE='FULLY_SQLTAINTED' WITH='UNSQLTAINTED' YIELDS='FULLY_SQLTAINTED' />
     <MERGE VALUE='FULLY_SQLTAINTED' WITH='MAYBE_SQLTAINTED' YIELDS='FULLY_SQLTAINTED' />
  </SUBTYPE>

  <SUBTYPE NAME='CheckHtmlTaint' ENABLE='FALSE' >
     <VALUE NAME='MAYBE_HTMLTAINTED' ATTRIBUTES='HtmlTainted Tainted' DEFAULT='TRUE' />
     <VALUE NAME='HTMLTAINTED' />
     <VALUE NAME='UNHTMLTAINTED' ATTRIBUTES='HtmlUntainted Untainted' CONSTANT='TRUE' UNINIT='TRUE' />

     <DEFAULT PRIMITIVE='TRUE' RESULT='UNHTMLTAINTED' />
     <DEFAULT CLASS='java.sql.Date' RESULT='UNHTMLTAINTED' />
     <DEFAULT CLASS='java.lang.Date' RESULT='UNHTMLTAINTED' />
     <DEFAULT CLASS='java.util.Number' RESULT='UNHTMLTAINTED' />
     <DEFAULT CLASS='java.util.Integer' RESULT='UNHTMLTAINTED' />
     <DEFAULT CLASS='java.util.Real' RESULT='UNHTMLTAINTED' />
     <DEFAULT CLASS='java.util.Float' RESULT='UNHTMLTAINTED' />
     <DEFAULT CLASS='java.util.Short' RESULT='UNHTMLTAINTED' />
     <DEFAULT CLASS='java.util.Byte' RESULT='UNHTMLTAINTED' />
     <DEFAULT CLASS='java.util.Long' RESULT='UNHTMLTAINTED' />
     <DEFAULT CLASS='java.util.Character' RESULT='UNHTMLTAINTED' />

     <BASE PRIMITIVE='TRUE' RESULT='UNHTMLTAINTED' />

     <OPERATION ARGS='HTMLTAINTED HTMLTAINTED' RESULT='HTMLTAINTED' VALUE='HTMLTAINTED' />
     <OPERATION OPERATOR='STARTINIT DONEINIT' VALUE='UNHTMLTAINTED' />
     <OPERATION AND='TRUE' ARGS='UNHTMLTAINTED UNHTMLTAINTED' VALUE='UNHTMLTAINTED' />
     <OPERATION METHOD='*' ARGS='HTMLTAINTED' RESULT='HTMLTAINTED' VALUE='HTMLTAINTED' />
     <OPERATION METHOD='*' AND='TRUE' ARGS='UNHTMLTAINTED' VALUE='UNHTMLTAINTED' />
     <OPERATION RETURN='RESULT' />

     <RESTRICT VALUE='HTMLTAINTED' WITH='UNHTMLTAINTED'
	ERROR='Attempt to use tainted HTML data in a non-tainted location' />
     <RESTRICT VALUE='HTMLTAINTED' WITH='MAYBE_HTMLTAINTED' YIELDS='HTMLTAINTED' />
     <RESTRICT VALUE='UNHTMLTAINTED' WITH='HTMLTAINTED' YIELDS='UNHTMLTAINTED' />
     <RESTRICT VALUE='UNHTMLTAINTED' WITH='MAYBE_HTMLTAINTED' YIELDS='UNHTMLTAINTED' />
     <RESTRICT VALUE='MAYBE_HTMLTAINTED' WITH='UNHTMLTAINTED'
	WARNING='Possible attempt to use tainted HTML data in a non-tainted location' />
     <RESTRICT VALUE='MAYBE_HTMLTAINTED' WITH='HTMLTAINTED' YIELDS='HTMLTAINTED' />

     <MERGE VALUE='HTMLTAINTED' WITH='UNHTMLTAINTED' YIELDS='HTMLTAINTED' />
     <MERGE VALUE='MAYBE_HTMLTAINTED' WITH='HTMLTAINTED' YIELDS='HTMLTAINTED' />
     <MERGE VALUE='MAYBE_HTMLTAINTED' WITH='UNHTMLTAINTED' YIELDS='MAYBE_HTMLTAINTED' />
  </SUBTYPE>

  <SUBTYPE NAME='CheckFileTaint' ENABLE='FALSE' >
     <VALUE NAME='MAYBE_FILETAINTED' ATTRIBUTES='FileTainted Tainted' DEFAULT='TRUE' />
     <VALUE NAME='FILETAINTED' />
     <VALUE NAME='UNFILETAINTED' ATTRIBUTES='FileUntainted Untainted' CONSTANT='TRUE' UNINIT='TRUE' />

     <DEFAULT PRIMITIVE='TRUE' RESULT='UNFILETAINTED' />
     <DEFAULT CLASS='java.sql.Date' RESULT='UNFILETAINTED' />
     <DEFAULT CLASS='java.lang.Date' RESULT='UNFILETAINTED' />
     <DEFAULT CLASS='java.util.Number' RESULT='UNFILETAINTED' />
     <DEFAULT CLASS='java.util.Integer' RESULT='UNFILETAINTED' />
     <DEFAULT CLASS='java.util.Real' RESULT='UNFILETAINTED' />
     <DEFAULT CLASS='java.util.Float' RESULT='UNFILETAINTED' />
     <DEFAULT CLASS='java.util.Short' RESULT='UNFILETAINTED' />
     <DEFAULT CLASS='java.util.Byte' RESULT='UNFILETAINTED' />
     <DEFAULT CLASS='java.util.Long' RESULT='UNFILETAINTED' />                                     E
     <DEFAULT CLASS='java.util.Character' RESULT='UNFILETAINTED' />

     <BASE PRIMITIVE='TRUE' RESULT='UNFILETAINTED' />

     <OPERATION ARGS='FILETAINTED FILETAINTED' RESULT='FILETAINTED' VALUE='FILETAINTED' />
     <OPERATION OPERATOR='STARTINIT DONEINIT' VALUE='UNFILETAINTED' />
     <OPERATION AND='TRUE' ARGS='UNFILETAINTED UNFILETAINTED' VALUE='UNFILETAINTED' />
     <OPERATION METHOD='*' ARGS='FILETAINTED' RESULT='FILETAINTED' VALUE='FILETAINTED' />
     <OPERATION METHOD='*' AND='TRUE' ARGS='UNFILETAINTED' VALUE='UNFILETAINTED' />
     <OPERATION RETURN='RESULT' />

     <RESTRICT VALUE='FILETAINTED' WITH='UNFILETAINTED'
	ERROR='Attempt to use tainted data in a file name' />
     <RESTRICT VALUE='FILETAINTED' WITH='MAYBE_FILETAINTED' YIELDS='FILETAINTED' />
     <RESTRICT VALUE='UNFILETAINTED' WITH='FILETAINTED' YIELDS='UNFILETAINTED' />
     <RESTRICT VALUE='UNFILETAINTED' WITH='MAYBE_FILETAINTED' YIELDS='UNFILETAINTED' />
     <RESTRICT VALUE='MAYBE_FILETAINTED' WITH='UNFILETAINTED'
	WARNING='Possible attempt to use tainted data in a file name' />
     <RESTRICT VALUE='MAYBE_FILETAINTED' WITH='FILETAINTED' YIELDS='FILETAINTED' />

     <MERGE VALUE='FILETAINTED' WITH='UNFILETAINTED' YIELDS='FILETAINTED' />
     <MERGE VALUE='MAYBE_FILETAINTED' WITH='FILETAINTED' YIELDS='FILETAINTED' />
     <MERGE VALUE='MAYBE_FILETAINTED' WITH='UNFILETAINTED' YIELDS='MAYBE_FILETAINTED' />
  </SUBTYPE>

  <SUBTYPE NAME='CheckTaint' ENABLE='FALSE' >
     <VALUE NAME='MAYBE_TAINTED' ATTRIBUTES='Tainted' DEFAULT='TRUE' />
     <VALUE NAME='TAINTED' />
     <VALUE NAME='UNTAINTED' ATTRIBUTES='Untainted' CONSTANT='TRUE' UNINIT='TRUE' />

     <DEFAULT PRIMITIVE='TRUE' RESULT='UNTAINTED' />

     <BASE PRIMITIVE='TRUE' RESULT='UNTAINTED' />

     <OPERATION ARGS='TAINTED TAINTED' RESULT='TAINTED' VALUE='TAINTED' />
     <OPERATION OPERATOR='STARTINIT DONEINIT' VALUE='UNTAINTED' />
     <OPERATION AND='TRUE' ARGS='UNTAINTED UNTAINTED' VALUE='UNTAINTED' />
     <OPERATION METHOD='*' ARGS='TAINTED' RESULT='TAINTED' VALUE='TAINTED' />
     <OPERATION METHOD='*' AND='TRUE' ARGS='UNTAINTED' VALUE='UNTAINTED' />
     <OPERATION RETURN='RESULT' />

     <RESTRICT VALUE='TAINTED' WITH='UNTAINTED'
	ERROR='Attempt to use tainted data in a non-tainted location'  />
     <RESTRICT VALUE='TAINTED' WITH='MAYBE_TAINTED' YIELDS='TAINTED' />
     <RESTRICT VALUE='UNTAINTED' WITH='TAINTED' YIELDS='UNTAINTED' />
     <RESTRICT VALUE='UNTAINTED' WITH='MAYBE_TAINTED' YIELDS='UNTAINTED' />
     <RESTRICT VALUE='MAYBE_TAINTED' WITH='UNTAINTED'
	WARNING='Possible attempt to use tainted data in a non-tainted location' />
     <RESTRICT VALUE='MAYBE_TAINTED' WITH='TAINTED' YIELDS='TAINTED' />

     <MERGE VALUE='TAINTED' WITH='UNTAINTED' YIELDS='TAINTED' />
     <MERGE VALUE='MAYBE_TAINTED' WITH='TAINTED' YIELDS='TAINTED' />
     <MERGE VALUE='MAYBE_TAINTED' WITH='UNTAINTED' YIELDS='MAYBE_TAINTED' />
  </SUBTYPE>

  <SUBTYPE NAME='CheckInitialization' ENABLE='FALSE' >
     <VALUE NAME='INITIALIZED' ATTRIBUTES='Initialized' CONSTANT='TRUE' />
     <VALUE NAME='UNKNOWN' ATTRIBUTES='UnknownInitialization' DEFAULT='TRUE' UNINIT='TRUE' />
     <VALUE NAME='UNDER_INITIALIZATION' ATTRIBUTES='UnderInitialization' />

     <OPERATION OPERATOR='FIELDACCESS ELEMENTACCESS DEREFERENCE' RETURN='RESULT' />
     <OPERATION OPERATOR='DONEINIT' RETURN='INITIALIZED' />
     <OPERATION OPERATOR='STARTINIT' RETURN='UNDER_INITIALIZATION' />
     <OPERATION RETURN='INITIALIZED' />

     <RESTRICT VALUE='INITIALIZED' WITH="UNKNWON" YIELDS='INITIALIZED' />
     <RESTRICT VALUE='INITIALIZED' WITH='UNDER_INITIALIZATION' YIELDS='INITIALIZED' />
     <RESTRICT VALUE='UNKNOWN' WITH='INITIALIZED' YIELDS='INITIALIZED' />
     <RESTRICT VALUE='UNKNOWN' WITH='UNDER_INITIALIZATION' YIELDS='UNDER_INITIALIZATION' />
     <RESTRICT VALUE='UNDER_INITIALIZATION' WITH='INITIALIZED' YIELDS='INITIALZED' />
     <RESTRICT VALUE='UNDER_INITIALIZATION' WITH='UNKNOWN' YIELDS='UNDER_INITIALZATION' />

     <MERGE VALUE='INITIALIZED' WITH='UNKNOWN' YIELDS='UNKNOWN' />
     <MERGE VALUE='INITIALIZED' WITH='UNDER_INITIALIZATION' YIELDS='UNKNOWN' />
     <MERGE VALUE='UNKNOWN' WITH='UNDER_INITIALIZATION' YIELDS='UNKNOWN' />
   </SUBTYPE>

</FAIT>































































